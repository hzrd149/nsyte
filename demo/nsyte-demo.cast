[44.142681, "o", "\u001b[36m\u001b[1mnsyte - Publish your site to nostr and blossom servers\u001b[0m\r\n\r\n"]
[44.143033, "o", "Usage: nsyte [command] [options]\r\n"]
[44.26263, "o", "\r\n"]
[44.38456, "o", "\u001b[33mCommands:\u001b[0m\r\n"]
[44.384641, "o", "  init       Initialize a new project configuration\r\n"]
[44.506531, "o", "  upload     Upload files to blossom servers\r\n"]
[44.629059, "o", "  ls         List files from nostr relays\r\n"]
[44.755662, "o", "  download   Download files from blossom servers\r\n"]
[44.87084, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[44.992134, "o", "\r\n"]
[45.112151, "o", "\u001b[33mOptions:\u001b[0m\r\n"]
[45.112233, "o", "  -h, --help     Display this help message\r\n"]
[45.230192, "o", "  -V, --version  Display version information\r\n"]
[45.353912, "o", "\r\n"]
[45.476373, "o", "\u001b[33mExamples:\u001b[0m\r\n  nsyte init             # Set up a new project\r\n"]
[45.59736, "o", "  nsyte upload .         # Upload current directory\r\n"]
[45.720138, "o", "  nsyte ls               # List published files\r\n"]
[45.846239, "o", "\r\n"]
