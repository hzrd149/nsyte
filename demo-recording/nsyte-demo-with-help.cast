[62.947124, "o", "nsyte - Publish your site to nostr and blossom servers\r\n"]
[63.113749, "o", "\r\n"]
[63.28321, "o", "Usage: nsyte [command] [options]\r\n"]
[63.449263, "o", "\r\n"]
[63.614456, "o", "Commands:\r\n"]
[63.775307, "o", "  init       Initialize a new project configuration\r\n"]
[63.940641, "o", "  upload     Upload files to blossom servers\r\n"]
[64.10526, "o", "  ls         List files from nostr relays\r\n"]
[64.275617, "o", "  download   Download files from blossom servers\r\n"]
[64.447082, "o", "  bunker     Connect to an nsec bunker\r\n"]
[64.617906, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[64.787477, "o", "\r\n"]
[64.962149, "o", "Options:\r\n"]
[65.133491, "o", "  -h, --help     Display this help message\r\n"]
[65.303601, "o", "  -v, --version  Display version information\r\n"]
[65.473469, "o", "\r\n"]
[65.64456, "o", "Examples:\r\n"]
[65.814846, "o", "  nsyte init             # Set up a new project\r\n"]
[65.97792, "o", "  nsyte upload .         # Upload current directory\r\n"]
[66.148197, "o", "  nsyte ls               # List published files\r\n"]
[66.321632, "o", "  nsyte bunker connect   # Connect to bunker\r\n"]
