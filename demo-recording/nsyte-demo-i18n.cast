[61.367913, "o", "\u001b[1m\u001b[36mnsyte - Publish your site to nostr and blossom servers\u001b[0m\r\n"]
[61.533395, "o", "\r\n"]
[61.70949, "o", "Usage: nsyte [command] [options]\r\n"]
[61.873645, "o", "\r\n"]
[62.11281, "o", "\u001b[33mCommands:\u001b[0m\r\n"]
[62.290809, "o", "  init       Initialize a new project configuration\r\n"]
[62.469708, "o", "  upload     Upload files to blossom servers\r\n"]
[62.652589, "o", "  ls         List files from nostr relays\r\n"]
[62.83394, "o", "  download   Download files from blossom servers\r\n"]
[63.017752, "o", "  bunker     Connect to an nsec bunker\r\n"]
[63.200109, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[63.382175, "o", "\r\n"]
[63.61581, "o", "\u001b[33mOptions:\u001b[0m\r\n"]
[63.797134, "o", "  -h, --help     Display this help message\r\n"]
[63.979304, "o", "  -v, --version  Display version information\r\n"]
[64.16243, "o", "\r\n"]
[64.400851, "o", "\u001b[33mExamples:\u001b[0m\r\n"]
[64.583185, "o", "  nsyte init             # Set up a new project\r\n"]
[64.767807, "o", "  nsyte upload .         # Upload current directory\r\n"]
[64.950012, "o", "  nsyte ls               # List published files\r\n"]
