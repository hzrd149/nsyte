[118.708335, "o", "\u001b[36m\u001b[1mnsyte - Publish your site to nostr and blossom servers\u001b[0m\r\n\r\n"]
[118.708401, "o", "Usage: nsyte [command] [options]\r\n"]
[118.928869, "o", "\r\n"]
[119.146784, "o", "\u001b[33mCommands:\u001b[0m\r\n"]
[119.146864, "o", "  init       Initialize a new project configuration\r\n"]
[119.365605, "o", "  upload     Upload files to blossom servers\r\n"]
[119.583768, "o", "  ls         List files from nostr relays\r\n"]
[119.801262, "o", "  download   Download files from blossom servers\r\n"]
[120.019631, "o", "  bunker     Connect to an nsec bunker\r\n"]
[120.23997, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[120.460583, "o", "\r\n"]
[120.67604, "o", "\u001b[33mOptions:\u001b[0m\r\n"]
[120.676144, "o", "  -h, --help     Display this help message\r\n"]
[120.890529, "o", "  -v, --version  Display version information\r\n"]
[121.107102, "o", "\r\n"]
[121.323911, "o", "\u001b[33mExamples:\u001b[0m\r\n  nsyte init             # Set up a new project\r\n"]
[121.539562, "o", "  nsyte upload .         # Upload current directory\r\n"]
[121.756216, "o", "  nsyte ls               # List published files\r\n"]
[121.974822, "o", "  nsyte bunker connect   # Connect to bunker\r\n"]
