[118.935073, "o", "\u001b[36m\u001b[1mnsyte - Publish your site to nostr and blossom servers\u001b[0m\r\n\r\n"]
[118.935213, "o", "Usage: nsyte [command] [options]\r\n"]
[119.155119, "o", "\r\n"]
[119.376249, "o", "\u001b[33mCommands:\u001b[0m\r\n"]
[119.37632, "o", "  init       Initialize a new project configuration\r\n"]
[119.594766, "o", "  upload     Upload files to blossom servers\r\n"]
[119.813359, "o", "  ls         List files from nostr relays\r\n"]
[120.03183, "o", "  download   Download files from blossom servers\r\n"]
[120.255448, "o", "  bunker     Connect to an nsec bunker\r\n"]
[120.480097, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[120.700694, "o", "\r\n"]
[120.916306, "o", "\u001b[33mOptions:\u001b[0m\r\n"]
[120.916384, "o", "  -h, --help     Display this help message\r\n"]
[121.13625, "o", "  -v, --version  Display version information\r\n"]
[121.351893, "o", "\r\n"]
[121.568436, "o", "\u001b[33mExamples:\u001b[0m\r\n"]
[121.568588, "o", "  nsyte init             # Set up a new project\r\n"]
[121.787155, "o", "  nsyte upload .         # Upload current directory\r\n"]
[122.004081, "o", "  nsyte ls               # List published files\r\n"]
[122.222219, "o", "  nsyte bunker connect   # Connect to bunker\r\n"]
