[19.564099, "o", "\u001b[36m\u001b[1mnsyte - Publish your site to nostr and blossom servers\u001b[0m\r\n\r\n"]
[19.564211, "o", "Usage: nsyte [command] [options]\r\n"]
[19.63057, "o", "\r\n"]
[19.698829, "o", "\u001b[33mCommands:\u001b[0m\r\n"]
[19.698937, "o", "  init       Initialize a new project configuration\r\n"]
[19.768806, "o", "  upload     Upload files to blossom servers\r\n"]
[19.837207, "o", "  ls         List files from nostr relays\r\n"]
[19.909451, "o", "  download   Download files from blossom servers\r\n"]
[19.98261, "o", "  bunker     Connect to an nsec bunker\r\n"]
[20.056902, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[20.128118, "o", "\r\n"]
[20.202807, "o", "\u001b[33mOptions:\u001b[0m\r\n"]
[20.202898, "o", "  -h, --help     Display this help message\r\n"]
[20.273651, "o", "  -v, --version  Display version information\r\n"]
[20.347244, "o", "\r\n"]
[20.416358, "o", "\u001b[33mExamples:\u001b[0m\r\n"]
[20.416426, "o", "  nsyte init             # Set up a new project\r\n"]
[20.484556, "o", "  nsyte upload .         # Upload current directory\r\n"]
[20.550339, "o", "  nsyte ls               # List published files\r\n"]
[20.614636, "o", "  nsyte bunker connect   # Connect to bunker\r\n"]
[20.677783, "o", "\r\n"]
