[123.486218, "o", "\u001b[36m\u001b[1mnsyte - Publish your site to nostr and blossom servers\u001b[0m\r\n\r\n"]
[123.48648, "o", "Usage: nsyte [command] [options]\r\n"]
[123.708569, "o", "\r\n"]
[123.939663, "o", "\u001b[33mCommands:\u001b[0m\r\n"]
[123.939841, "o", "  init       Initialize a new project configuration\r\n"]
[124.177557, "o", "  upload     Upload files to blossom servers\r\n"]
[124.402179, "o", "  ls         List files from nostr relays\r\n"]
[124.635725, "o", "  download   Download files from blossom servers\r\n"]
[124.866617, "o", "  bunker     Connect to an nsec bunker\r\n"]
[125.094401, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[125.325235, "o", "\r\n"]
[125.543689, "o", "\u001b[33mOptions:\u001b[0m\r\n"]
[125.543756, "o", "  -h, --help     Display this help message\r\n"]
[125.775251, "o", "  -v, --version  Display version information\r\n"]
[126.00743, "o", "\r\n"]
[126.230312, "o", "\u001b[33mExamples:\u001b[0m\r\n"]
[126.230364, "o", "  nsyte init             # Set up a new project\r\n"]
[126.456174, "o", "  nsyte upload .         # Upload current directory\r\n"]
[126.689623, "o", "  nsyte ls               # List published files\r\n"]
[126.919736, "o", "  nsyte bunker connect   # Connect to bunker\r\n"]
[127.154526, "o", "\r\n"]
