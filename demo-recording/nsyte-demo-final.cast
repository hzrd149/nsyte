[62.548701, "o", "nsyte - Publish your site to nostr and blossom servers\r\n"]
[62.719832, "o", "\r\n"]
[62.889826, "o", "Usage: nsyte [command] [options]\r\n"]
[63.05712, "o", "\r\n"]
[63.223904, "o", "Commands:\r\n"]
[63.389574, "o", "  init       Initialize a new project configuration\r\n"]
[63.555358, "o", "  upload     Upload files to blossom servers\r\n"]
[63.721056, "o", "  ls         List files from nostr relays\r\n"]
[63.881872, "o", "  download   Download files from blossom servers\r\n"]
[64.049208, "o", "  bunker     Connect to an nsec bunker\r\n"]
[64.217443, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[64.382867, "o", "\r\n"]
[64.546219, "o", "Options:\r\n"]
[64.714271, "o", "  -h, --help     Display this help message\r\n"]
[64.884108, "o", "  -v, --version  Display version information\r\n"]
[65.053211, "o", "\r\n"]
[65.221176, "o", "Examples:\r\n"]
[65.389085, "o", "  nsyte init             # Set up a new project\r\n"]
[65.55191, "o", "  nsyte upload .         # Upload current directory\r\n"]
[65.721645, "o", "  nsyte ls               # List published files\r\n"]
[65.89183, "o", "  nsyte bunker connect   # Connect to bunker\r\n"]
