[19.924744, "o", "\u001b[36m\u001b[1mnsyte - Publish your site to nostr and blossom servers\u001b[0m\r\n\r\n"]
[19.924852, "o", "Usage: nsyte [command] [options]\r\n"]
[19.990929, "o", "\r\n"]
[20.053015, "o", "\u001b[33mCommands:\u001b[0m\r\n"]
[20.053129, "o", "  init       Initialize a new project configuration\r\n"]
[20.11937, "o", "  upload     Upload files to blossom servers\r\n"]
[20.185865, "o", "  ls         List files from nostr relays\r\n"]
[20.252365, "o", "  download   Download files from blossom servers\r\n"]
[20.319198, "o", "  bunker     Connect to an nsec bunker\r\n"]
[20.385573, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[20.449413, "o", "\r\n"]
[20.511346, "o", "\u001b[33mOptions:\u001b[0m\r\n"]
[20.511452, "o", "  -h, --help     Display this help message\r\n"]
[20.578013, "o", "  -v, --version  Display version information\r\n"]
[20.644485, "o", "\r\n"]
[20.711519, "o", "\u001b[33mExamples:\u001b[0m\r\n"]
[20.71156, "o", "  nsyte init             # Set up a new project\r\n"]
[20.778685, "o", "  nsyte upload .         # Upload current directory\r\n"]
[20.845214, "o", "  nsyte ls               # List published files\r\n"]
[20.91112, "o", "  nsyte bunker connect   # Connect to bunker\r\n"]
[20.97726, "o", "\r\n"]
