[63.772237, "o", "\u001b[36m\u001b[1mnsyte - Publish your site to nostr and blossom servers\u001b[0m\r\n\r\n"]
[63.772322, "o", "Usage: nsyte [command] [options]\r\n"]
[63.93745, "o", "\r\n"]
[64.102962, "o", "\u001b[33mCommands:\u001b[0m\r\n"]
[64.102998, "o", "  init       Initialize a new project configuration\r\n"]
[64.268184, "o", "  upload     Upload files to blossom servers\r\n"]
[64.433382, "o", "  ls         List files from nostr relays\r\n"]
[64.598554, "o", "  download   Download files from blossom servers\r\n"]
[64.76362, "o", "  bunker     Connect to an nsec bunker\r\n"]
[64.928858, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[65.094471, "o", "\r\n"]
[65.259386, "o", "\u001b[33mOptions:\u001b[0m\r\n"]
[65.259414, "o", "  -h, --help     Display this help message\r\n"]
[65.424251, "o", "  -v, --version  Display version information\r\n"]
[65.588855, "o", "\r\n"]
[65.75491, "o", "\u001b[33mExamples:\u001b[0m\r\n"]
[65.754926, "o", "  nsyte init             # Set up a new project\r\n"]
[65.919002, "o", "  nsyte upload .         # Upload current directory\r\n"]
[66.084246, "o", "  nsyte ls               # List published files\r\n"]
[66.248426, "o", "  nsyte bunker connect   # Connect to bunker\r\n"]
[66.413427, "o", "\r\n"]
