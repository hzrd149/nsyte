[43.162436, "o", "\u001b[36m\u001b[1mnsyte - Publish your site to nostr and blossom servers\u001b[0m\r\n\r\n"]
[43.162538, "o", "Usage: nsyte [command] [options]\r\n"]
[43.281422, "o", "\r\n"]
[43.403491, "o", "\u001b[33mCommands:\u001b[0m\r\n"]
[43.403569, "o", "  init       Initialize a new project configuration\r\n"]
[43.526805, "o", "  upload     Upload files to blossom servers\r\n"]
[43.653832, "o", "  ls         List files from nostr relays\r\n"]
[43.77596, "o", "  download   Download files from blossom servers\r\n"]
[43.896092, "o", "  ci         Generate CI/CD-friendly bunker connection\r\n"]
[44.012107, "o", "\r\n"]
[44.130789, "o", "\u001b[33mOptions:\u001b[0m\r\n"]
[44.130889, "o", "  -h, --help     Display this help message\r\n"]
[44.254964, "o", "  -V, --version  Display version information\r\n"]
[44.378732, "o", "\r\n"]
[44.503616, "o", "\u001b[33mExamples:\u001b[0m\r\n"]
[44.503999, "o", "  nsyte init             # Set up a new project\r\n"]
[44.630558, "o", "  nsyte upload .         # Upload current directory\r\n"]
[44.753259, "o", "  nsyte ls               # List published files\r\n"]
[44.872736, "o", "\r\n"]
